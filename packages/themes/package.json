{
  "name": "@nugudi/themes",
  "version": "0.0.1",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./themes.css": "./dist/themes.css"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "pnpm run clean && tsx build.config.ts && pnpm run build:types",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rm -rf dist",
    "dev": "tsx watch build.config.ts"
  },
  "devDependencies": {
    "esbuild": "^0.20.0",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3"
  }
}
